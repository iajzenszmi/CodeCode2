// program to list x and cos(x) ie y values.
//by I.M. Ajzenszmidt
#include <cmath>
#include <cstdio>
#include <cstdlib>
#include <iostream>
int main(){
 float y;
 float x;
std::cout << "program to list cosine values " << std::endl;
std::cout << "by I.M. Ajzenszmidt " << std::endl;
 for(x=1;x<=10;x++){
 y=cos(x);
  std::cout << x<<" "<<y << std::endl;
 }
}
ian@ian-HP-Stream-Laptop-11-y0XX:~$ g++  coslist.cpp -lm  -o coslist
